doctype html
html.no-js.css-menubar(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui')
    meta(name='description', content='BitRain')
    meta(name='author', content='')
    title #{title} - BitRain
    link(rel='apple-touch-icon', href='/assets/images/apple-touch-icon.png')
    link(rel='shortcut icon', href='/assets/images/favicon.ico')


    link(rel='stylesheet', href='/global/css/bootstrap.min.css')
    link(rel='stylesheet', href='/global/css/bootstrap-extend.min.css')
    //link(rel='stylesheet', href='/global/css/v1.min.css')
    link(rel='stylesheet', href='/assets/css/font-awesome.min.css')
    link(rel='stylesheet', href='/assets/css/site.min.css')
    link(rel='stylesheet', href='/css/style.css?v=3')
    link(rel='stylesheet', href='/assets/css/qrtip-1.0-jquery.css')
    link(rel='stylesheet', href='/assets/css/960.css')
    // Plugins
    link(rel='stylesheet', href='/global/vendor/animsition/animsition.css')
    link(rel='stylesheet', href='/global/vendor/asscrollable/asScrollable.css')
    link(rel='stylesheet', href='/global/vendor/switchery/switchery.css')
    link(rel='stylesheet', href='/global/vendor/intro-js/introjs.css')
    link(rel='stylesheet', href='/global/vendor/slidepanel/slidePanel.css')
    link(rel='stylesheet', href='/global/vendor/jquery-mmenu/jquery-mmenu.css')
    link(rel='stylesheet', href='/global/vendor/flag-icon-css/flag-icon.css')
    link(rel='stylesheet', href='/global/vendor/waves/waves.css')
    link(rel='stylesheet', href='/global/vendor/jquery-mmenu/jquery-mmenu.css')
    block css_plugins
    block page_css
    - if (pageType)
      link(rel='stylesheet', href='/assets/css/' + pageType + '.css?v=3')
    // Fonts
    link(rel='stylesheet', href='/global/fonts/material-design/material-design.min.css')
    link(rel='stylesheet', href='/global/fonts/brand-icons/brand-icons.min.css')
    link(href='https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i&subset=latin-ext,vietnamese', rel='stylesheet')
    //if lt IE 9
      script(src='/global/vendor/html5shiv/html5shiv.min.js')
    //if lt IE 10
      script(src='/global/vendor/media-match/media.match.min.js')

      script(src='/global/vendor/respond/respond.min.js')
    // Scripts
    script(src='/global/vendor/breakpoints/breakpoints.js')
    script.
      Breakpoints();
    script(src='/global/vendor/babel-external-helpers/babel-external-helpers.js')
    script(src='/global/vendor/jquery/jquery.js')
  body.animsition.dashboard

    //include partials/navigation
    include partials/site-menubar

    // Page
    .page
      - if (showHeader)
        .page-header
          h1.page-tile #{title}
      .page-content.container-fluid
        .banner-wrapper.bg-style.bg-home
          .col-md-12
            .content-banner
              .icon
                //img.profile(data-name=user.full_nm, alt=user.full_nm, style="border-radius:50%;width:60px;border: 1px solid white", src=user.avatar())
                .absolute(style='right: 8px; top:47px')
                  span.imagespinner.imagespinning.kyc(style='right: 15px; left: auto')
              .title
                span Hi
                | ,
                //span.username  #{user.full_nm}
        .content-wrapper
          include partials/flash
          block content
    // End Page
    // Footer
    footer.site-footer
      .site-footer-legal
        | Â© 2016
        a(href='http://www.bitrain.info') BitRain
      .site-footer-right
        | Crafted with
        i.red-600.icon.md-favorite
        |  by
        a(href='http://www.bitrain.info') BitRain Commonity.
    script(src='/global/vendor/tether/tether.js')
    script(src='/global/vendor/bootstrap/bootstrap.js')
    script(src='/global/vendor/animsition/animsition.js')
    script(src='/global/vendor/mousewheel/jquery.mousewheel.js')
    script(src='/global/vendor/asscrollbar/jquery-asScrollbar.js')
    script(src='/global/vendor/asscrollable/jquery-asScrollable.js')
    script(src='/global/vendor/waves/waves.js')
    // Plugins
    script(src='/global/vendor/jquery-mmenu/jquery.mmenu.min.all.js')
    script(src='/global/vendor/switchery/switchery.min.js')
    script(src='/global/vendor/intro-js/intro.js')
    script(src='/global/vendor/screenfull/screenfull.js')
    script(src='/global/vendor/slidepanel/jquery-slidePanel.js')
    block js_plugins
    // Scripts
    script(src='/global/js/State.js')
    script(src='/global/js/Component.js')
    script(src='/global/js/Plugin.js')
    script(src='/global/js/Base.js')
    script(src='/global/js/Config.js')
    script(src='/assets/js/Section/Menubar.js')
    script(src='/assets/js/Section/Sidebar.js')
    script(src='/assets/js/Section/PageAside.js')
    script(src='/assets/js/Plugin/menu.js')
    script(src='/assets/js/qrtip-1.0-jquery.js')
    // Config
    script(src='/global/js/config/colors.js')
    script(src='/assets/js/config/tour.js')
    script.
      Config.set('assets', '/assets');
    // Page
    script(src='/assets/js/Site.js')
    script(src='/assets/js/app.js')
    script(src='/global/js/Plugin/asscrollable.js')
    script(src='/global/js/Plugin/slidepanel.js')
    script(src='/global/js/Plugin/switchery.js')
    script(src='/js/initial.min.js')
    script(src='/js/main.js?v=3')
    block scripts